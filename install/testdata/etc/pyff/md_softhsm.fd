- load:
  - ../testdata/md_source
- select
- xslt:
    stylesheet: tidy.xsl
- store: 
     directory: /tmp/
- finalize:
     cacheDuration: PT4H
     validUntil: P10D
     ID: your_metadatafeed_prefix_
- sign:
    # the last part of the "URI" is the key name on the eToken
    key: pkcs11:///usr/lib64/libsofthsm2.so/test
    # Token PW from ENV PYKCS11PIN
- publish: ../testdata/md_feed/metadata_softhsm.xml